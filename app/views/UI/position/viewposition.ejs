<h3>User</h3>
<form action="/devpos/<%=pageData.action%>" method="POST">

        <input type="hidden" id="rowid" name="rowid" value="<%=result.rowid%>" >   
    <div class="form-group row">
        <label for="room" class="col-sm-2 col-form-label">Room</label>
        <div class="col-sm-10">
            <input type="text" class="form-control"  disabled
            value="<%=result.room%>" >
        </div>
    </div>
    <div class="form-group row">
        <label for="label" class="col-sm-2 col-form-label">Label</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="label" name="label" 
            value="<%=result.label%>" placeholder="Label">
        </div>
    </div>
    <div class="form-group row">
        <label for="address" class="col-sm-2 col-form-label">Block</label>
        <div class="col-sm-10">
            <input type="text" class="form-control"
            value="<%=result.block%>" disabled >
          
        </div>
    </div>
    <div class="form-group row">
        <label for="floor" class="col-sm-2 col-form-label">Floor</label>
        <div class="col-sm-10">
            <input type="text" class="form-control"
            value="<%=result.floor%>" disabled >
          
        </div>
    </div>
   
    <div class="form-group row">
        <label for="roomtype" class="col-sm-2 col-form-label">Room Type</label>
        <div class="col-sm-10">
         <select class="form-control" id="roomtype" name="roomtype" disabled
          value="<%=result.roomtype%>">
          <option value="" selected >Select </option>
          <option value="DEFAULT" >Default</option> 
          <option value="LIVING">Living Room</option>
          <option value="HALL" >Hall</option> 
          <option value="BED" >Bed Room</option> 
          <option value="LAWN" >Lawn</option> 
          <option value="KITCHEN" >Kitchen</option> 
          <option value="GARAGE">Garage</option>
          <option value="OTH">Other</option>
         </select>
        </div>
    </div>
    <div class="form-group row">
        <label for="hor_pos" class="col-sm-2 col-form-label">Position: Horizontal</label>
        <div class="col-sm-2">
             <select class="form-control" id="hor_pos" name="hor_pos" disabled
                value="<%=result.hor_pos%>" >
                <option value="" selected>Select </option>
                <option value="OTH">Other</option>
                <option value="E" >East</option> 
                <option value="W">West </option>
                <option value="N" >North</option> 
                <option value="S" >South</option> 
                <option value="SE" >South East</option> 
                <option value="SW" >South West</option> 
                <option value="NE">Norht East</option>
                <option value="NW">North West</option>
               </select>
        </div>

            <label for="ver_pos" class="col-sm-1 col-form-label">Vertical </label>
            <div class="col-sm-2">
              <select class="form-control" id="ver_pos" name="ver_pos" disabled
                value="<%=result.hor_pos%>">
                <option value="" selected >Select </option>
                <option value="OTH">Ohter</option>
                <option value="UP" >Up</option> 
                <option value="MID" >Middle</option> 
                <option value="DN">Down </option>
                <option value="FL" >Floor</option> 
                <option value="CE" >Ceiling</option>                
               </select>
            </div>
        </div>
        <div class="form-group row">
            <label for="type_pos" class="col-sm-2 col-form-label">Position Type</label>
            <div class="col-sm-10">
             <select class="form-control" id="type_pos" name="type_pos" disabled
              value="<%=result.type_pos%>">
              <option value="" selected >Select </option>
              <option value="DEFAULT" >Default</option> 
              <option value="ROOF">Roof/Terrace</option>
              <option value="FLOOR" >Floor</option>    
              <option value="CELLAR" >Cellar</option>            
             </select>
            </div>
        </div>
        <div class="form-group row">
            <label for="latitude" class="col-sm-2 col-form-label">Latitude</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" 
                value="<%=result.lat%>" disabled>
            </div>
        </div>
        <div class="form-group row">
            <label for="lng" class="col-sm-2 col-form-label">Longitude</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" 
                value="<%=result.lng%>" disabled>
            </div>
        </div>
 
        
        <div class="form-group row">
            <div class="col-sm-10 offset-sm-2">  
                <%if(pageData.action==="delete"  && user.usertype==="ADMIN"){%>                
                <button type="submit" class="btn btn-primary">Delete</button>
                <%}%>
                <%if(pageData.mode==="view" && user.usertype==="ADMIN"){%>                
                    <a href="/<%=pageData.urlRoute%>/edit/<%=result.rowid%>" class="btn btn-primary">Edit</a>
                <%}%>
              
                <a href="javascript:history.back()"  class="btn btn-primary">Back</a>
            </div>
        </div>
</form>

<script>
    $(function() {
        $("#roomtype").val("<%=result.roomtype%>");
        $("#type_pos").val("<%=result.type_pos%>");  
        $("#hor_pos").val("<%=result.hor_pos%>");
        $("#ver_pos").val("<%=result.ver_pos%>"); 
    });
  </script>